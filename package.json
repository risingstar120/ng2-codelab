    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "monaco": "^1.201612240510.0",
    "monaco-editor": "^0.7.0",
    "node-polyglot": "^2.2.2",
    "node-ts": "^2.1.2",
    "plugin-typescript": "^5.3.1",
    "protractor": "~4.0.13",
    "systemjs": "^0.19.45",
    "systemjs-builder": "^0.16.0",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
